{
  "name": "@preview-workspace/preview-lib",
  "version": "2.2.2",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "http://ec2-13-53-35-90.eu-north-1.compute.amazonaws.com:4873/"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist",
    "prepublish": "npm run clean && npm run build",
    "prepublishOnly": "npm run prepublish",
    "publish:patch": "npm run prepublish && npm version patch && npm publish",
    "publish:minor": "npm run prepublish && npm version minor && npm publish",
    "publish:major": "npm run prepublish && npm version major && npm publish",
    "publish:local": "npm run prepublish && npm publish --registry http://localhost:4873/",
    "publish:local:patch": "npm run prepublish && npm version patch && npm publish --registry http://localhost:4873/",
    "publish:local:minor": "npm run prepublish && npm version minor && npm publish --registry http://localhost:4873/",
    "publish:local:major": "npm run prepublish && npm version major && npm publish --registry http://localhost:4873/",
    "verdaccio:start": "verdaccio --config .verdaccio/config.yml",
    "verdaccio:stop": "pkill -f verdaccio"
  },
  "private": false,
  "dependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/js": "^9.8.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^3.0.0",
    "ajv": "^8.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "verdaccio": "^6.0.5",
    "vite": "^6.0.0",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "^3.0.0"
  }
}
